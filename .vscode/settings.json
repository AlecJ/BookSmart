{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "editor.colorDecorators": true,
  "colorize.languages": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "colorize.colorized_colors": [
    "BROWSERS_COLORS",
    "HEXA",
    "RGB",
    "HSL"
  ],
  "editor.quickSuggestions": {
    "strings": true
  }
}
